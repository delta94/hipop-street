{
  "name": "MusicApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "tslint --project . -e '**/*.js' -t verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.tsx?$' | xargs tslint | snazzy",
    "fixcode": "tslint --project . -e '**/*.js' --fix",
    "git-hook": "npm run lint -s && npm run test -s"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^1.1.1",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/clipboard": "^1.2.2",
    "@react-native-community/datetimepicker": "^2.3.0",
    "@react-native-community/google-signin": "^3.0.4",
    "@react-native-community/slider": "^2.0.8",
    "@types/react-native-fbsdk": "^1.0.0",
    "@types/react-native-push-notification": "^3.0.8",
    "@types/react-native-share": "^3.0.0",
    "@types/react-native-video": "^5.0.0",
    "apisauce": "^0.14.0",
    "format-json": "^1.0.3",
    "jsc-android": "^241213.1.0",
    "lodash": "^4.17.2",
    "native-base": "^2.13.8",
    "querystringify": "0.0.4",
    "ramda": "^0.24.1",
    "react": "^16.12.0",
    "react-native": "0.61.0",
    "react-native-device-info": "^5.6.1",
    "react-native-document-picker": "^3.2.4",
    "react-native-drawer": "^2.3.0",
    "react-native-fbsdk": "^1.1.1",
    "react-native-firebase": "^5.6.0",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-google-shortener": "^1.1.0",
    "react-native-image-picker": "^2.0.0",
    "react-native-push-notification": "^3.1.9",
    "react-native-safe-area-context": "^1.0.2",
    "react-native-screens": "^2.8.0",
    "react-native-share": "^3.0.0",
    "react-native-sound-player": "^0.10.3",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swift": "^1.2.3",
    "react-native-track-player": "^2.0.0-rc13",
    "react-native-vector-icons": "^6.6.0",
    "react-native-video": "^5.0.2",
    "react-native-webview": "^8.1.2",
    "react-navigation": "3.12.0",
    "react-redux": "^5.0.2",
    "react-redux-typescript": "^2.3.0",
    "redux": "^3.6.0",
    "redux-observable": "^1.2.0",
    "redux-persist": "^4.1.0",
    "redux-saga": "^0.15.6",
    "rxjs": "^6.5.3",
    "seamless-immutable": "^7.0.1",
    "shorthash": "0.0.2",
    "typesafe-actions": "1.1.2"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.2.3",
    "@storybook/react-native": "^3.2.3",
    "@types/enzyme": "^3.1.0",
    "@types/jest": "^21.1.4",
    "@types/ramda": "^0.24.17",
    "@types/react": "^16.0.13",
    "@types/react-native": "^0.49.2",
    "@types/react-navigation": "^1.0.21",
    "@types/react-redux": "^5.0.10",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux": "^3.6.0",
    "@types/seamless-immutable": "^7.1.1",
    "@types/storybook__react": "^3.0.5",
    "@types/webpack-env": "^1.13.2",
    "babel-jest": "21.2.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "3.0.2",
    "enzyme": "^2.6.0",
    "husky": "^0.13.1",
    "ignite-animatable": "^1.0.0",
    "ignite-dev-screens": "^2.2.0",
    "ignite-typescript-boilerplate": "^0.1.5",
    "ignite-vector-icons": "^1.1.0",
    "jest": "^21.2.1",
    "jest-preset-typescript-react-native": "^1.2.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "^0.57.0",
    "mockery": "^2.0.0",
    "react-addons-test-utils": "~15.4.1",
    "react-dom": "16.0.0-alpha.12",
    "react-native-typescript-transformer": "^1.1.4",
    "react-test-renderer": "16.0.0-beta.5",
    "reactotron-react-native": "^3.6.0",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1",
    "redux-logger": "^3.0.6",
    "snazzy": "^7.0.0",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.3"
  },
  "jest": {
    "preset": "jest-preset-typescript-react-native",
    "testMatch": [
      "**/Tests/**/*.ts?(x)",
      "**/App/**/*Test.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "Tests/Setup"
    ],
    "setupFiles": [
      "./Tests/Setup.tsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "config": {}
}
